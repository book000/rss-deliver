{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.7.149",
    "@book000/node-utils": "1.13.459",
    "@types/cheerio": "0.22.35",
    "@types/node": "22.13.2",
    "axios": "1.7.9",
    "canvas": "3.1.0",
    "cheerio": "1.0.0",
    "eslint": "9.20.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.15.1",
    "eslint-plugin-promise": "7.2.1",
    "fast-xml-parser": "4.5.1",
    "pdfjs-dist": "4.10.38",
    "prettier": "3.5.0",
    "run-z": "2.1.0",
    "sharp": "0.33.5",
    "tsx": "4.19.2",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.3.0+sha512.ee592eda8815a8a293c206bb0917c4bb0ff274c50def7cbc17be05ec641fc2d1b02490ce660061356bd0d126a4d7eb2ec8830e6959fb8a447571c631d5a2442d",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild",
      "sharp"
    ]
  }
}
