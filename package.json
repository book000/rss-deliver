{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.9.9",
    "@book000/node-utils": "1.18.27",
    "@types/cheerio": "0.22.35",
    "@types/node": "22.15.32",
    "axios": "1.10.0",
    "canvas": "3.1.1",
    "cheerio": "1.0.0",
    "eslint": "9.29.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.20.0",
    "eslint-plugin-promise": "7.2.1",
    "fast-xml-parser": "5.2.5",
    "pdfjs-dist": "5.3.31",
    "prettier": "3.5.3",
    "run-z": "2.1.0",
    "sharp": "0.34.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.12.1+sha512.f0dda8580f0ee9481c5c79a1d927b9164f2c478e90992ad268bbb2465a736984391d6333d2c327913578b2804af33474ca554ba29c04a8b13060a717675ae3ac",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild",
      "sharp"
    ]
  }
}
