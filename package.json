{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.12.42",
    "@book000/node-utils": "1.24.38",
    "@types/node": "24.10.9",
    "axios": "1.13.2",
    "canvas": "3.2.1",
    "cheerio": "1.1.2",
    "eslint": "9.39.2",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-n": "17.23.2",
    "eslint-plugin-promise": "7.2.1",
    "fast-xml-parser": "5.3.3",
    "pdfjs-dist": "5.4.530",
    "prettier": "3.8.1",
    "run-z": "2.1.0",
    "sharp": "0.34.5",
    "tsx": "4.21.0",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.28.1+sha512.7d7dbbca9e99447b7c3bf7a73286afaaf6be99251eb9498baefa7d406892f67b879adb3a1d7e687fc4ccc1a388c7175fbaae567a26ab44d1067b54fcb0d6a316",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild",
      "sharp"
    ]
  }
}
