{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.7.150",
    "@book000/node-utils": "1.14.90",
    "@types/cheerio": "0.22.35",
    "@types/node": "22.15.0",
    "axios": "1.9.0",
    "canvas": "3.1.0",
    "cheerio": "1.0.0",
    "eslint": "9.25.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "17.17.0",
    "eslint-plugin-promise": "7.2.1",
    "fast-xml-parser": "5.2.1",
    "pdfjs-dist": "5.1.91",
    "prettier": "3.5.3",
    "run-z": "2.1.0",
    "sharp": "0.34.1",
    "tsx": "4.19.3",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.9.0+sha512.0486e394640d3c1fb3c9d43d49cf92879ff74f8516959c235308f5a8f62e2e19528a65cdc2a3058f587cde71eba3d5b56327c8c33a97e4c4051ca48a10ca2d5f",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    },
    "onlyBuiltDependencies": [
      "canvas",
      "esbuild",
      "sharp"
    ]
  }
}
