{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . -c eslint.config.mjs --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . -c eslint.config.mjs",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/eslint-config": "1.7.28",
    "@book000/node-utils": "1.13.201",
    "@types/cheerio": "0.22.35",
    "@types/node": "20.14.13",
    "axios": "1.7.2",
    "canvas": "2.11.2",
    "cheerio": "1.0.0-rc.12",
    "eslint": "9.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.10.1",
    "eslint-plugin-promise": "7.0.0",
    "fast-xml-parser": "4.4.1",
    "pdfjs-dist": "4.5.136",
    "prettier": "3.3.3",
    "run-z": "2.0.0",
    "sharp": "0.33.4",
    "tsx": "4.16.4",
    "typescript": "5.5.4"
  },
  "packageManager": "pnpm@9.6.0",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  }
}
