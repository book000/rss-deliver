{
  "name": "rss-deliver",
  "version": "2.0.0",
  "description": "RSS collector & deliver for myself",
  "homepage": "https://github.com/book000/rss-deliver",
  "bugs": {
    "url": "https://github.com/book000/rss-deliver/issues"
  },
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git@github.com:book000/rss-deliver.git"
  },
  "scripts": {
    "start": "tsx ./src/main.ts",
    "dev": "tsx watch ./src/main.ts",
    "fix": "run-z fix:prettier,fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src",
    "lint": "run-z lint:prettier,lint:eslint,lint:tsc",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:prettier": "prettier --check src",
    "lint:tsc": "tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@book000/node-utils": "1.12.123",
    "@types/cheerio": "0.22.35",
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.6.0",
    "@typescript-eslint/parser": "7.6.0",
    "axios": "1.6.8",
    "canvas": "2.11.2",
    "cheerio": "1.0.0-rc.12",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "17.2.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "52.0.0",
    "fast-xml-parser": "4.3.6",
    "pdfjs-dist": "4.1.392",
    "prettier": "3.2.5",
    "run-z": "2.0.0",
    "sharp": "0.33.3",
    "tsx": "4.7.2",
    "typescript": "5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "pnpm": {
    "supportedArchitectures": {
      "os": [
        "win32",
        "darwin",
        "current"
      ],
      "cpu": [
        "x64",
        "arm64"
      ]
    }
  }
}
